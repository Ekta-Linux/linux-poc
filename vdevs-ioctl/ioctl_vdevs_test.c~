#include <stdio.h>
#include <fcntl.h>
#include <sys/ioctl.h>
#include <unistd.h>
#include "platform.h"

#define DEV_NAME    "/dev/vDev-0"

int main() {
	
	int fd = open(DEV_NAME, O_RDWR);
	if (fd < 0) {
		perror("open");
		return -1;
	}

	//ioctl(fd, VDEV_FILLCHAR, 'A');
	ioctl(fd, VDEV_FILLZERO);

	close(fd);

	return 0; 
}
